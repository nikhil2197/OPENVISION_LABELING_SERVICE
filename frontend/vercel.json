{
  "rewrites": [
    {
      "source": "/api/snip",
      "destination": "https://openvision-labeling-service.onrender.com/api/snip"
    },
    {
      "source": "/api/cleanup-session/:sessionId",
      "destination": "https://openvision-labeling-service.onrender.com/api/cleanup-session/:sessionId"
    },
    {
      "source": "/api/upload-url",
      "destination": "https://openvision-labeling-service.onrender.com/api/upload-url"
    },
    {
      "source": "/api/download-url",
      "destination": "https://openvision-labeling-service.onrender.com/api/download-url"
    }
  ],
  "env": {
    "BACKEND_URL": "https://openvision-labeling-service.onrender.com"
  }
} 